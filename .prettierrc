{
  "printWidth": 80,
  "tabWidth": 4,
  "useTabs": true,
  "semi": false,
  "singleQuote": false,
  "quoteProps": "as-needed",
  "jsxSingleQuote": false,
  "trailingComma": "all",
  "bracketSpacing": true,
  "bracketSameLine": false,
  "arrowParens": "always",
  "rangeStart": 0,
  "parser": "typescript",
  "requirePragma": true,
  "insertPragma": true,
  "endOfLine": "lf",
  "singleAttributePerLine": true
}
